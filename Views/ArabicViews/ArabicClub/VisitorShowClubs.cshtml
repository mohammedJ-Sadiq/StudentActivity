@model IEnumerable<StudentActivity.Models.Club>

@{
    ViewBag.Title = "VisitorShowClubs";
    Layout = "~/Views/Shared/ArabicLayout/_ArabicLayout.cshtml";

    <div>
        <ul class="breadcrumb">
            <li>@Html.ActionLink(StudentActivity.Resources.Language.Home, "Index", "Home")</li>
            <li>@Html.ActionLink(StudentActivity.Resources.Language.Clubs, "VisitorShowClubs", "Club")</li>
        </ul>
    </div>
}

<table class="table table-striped table-hover">
    <tr>
        <th>
            @StudentActivity.Resources.Language.Club_Name
        </th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @item.Name
            </td>
        </tr>
    }

</table>