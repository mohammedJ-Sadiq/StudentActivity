@model IEnumerable<StudentActivity.Models.Student_Club>
@{
    ViewBag.Title = "ShowClubs";
    Layout = "~/Views/Shared/_StudentLayout.cshtml";
}

<h2></h2>

<p>
    @Html.ActionLink("+ Join a club", "AddStuClubs", "Club", new { @class = "btn btn-blue btn-left" })
</p>
<table class="table table-striped table-hover">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Club.Name)
        </th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @item.Club.Name
            </td>
        </tr>
    }

</table>